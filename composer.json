{
    "name": "inspetor/inspetor-php",
    "description": "Composer for Antifraud Inspetor PHP library installation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Theo Sato",
            "email": "theo@useinspetor.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Inspetor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inspetor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "phalcon/incubator": "3.4.*",
        "snowplow/snowplow-tracker": "0.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    }
}
